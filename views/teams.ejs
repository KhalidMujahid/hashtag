<!-- ======= Team Section ======= -->
<section id="team" class="team">
  <div class="container" data-aos="fade-up">
    <div class="section-header">
      <h2>Our Team</h2>
      <p>
        Architecto nobis eos vel nam quidem vitae temporibus voluptates qui hic
        deserunt iusto omnis nam voluptas asperiores sequi tenetur dolores
        incidunt enim voluptatem magnam cumque fuga.
      </p>
    </div>

    <div class="row gy-5">
      <%# Team array %> <% const teams =
      ["team-1.jpg","team-2.jpg","team-3.jpg"] %> <% teams.forEach((team) => {%>
      <div
        class="col-xl-4 col-md-6 d-flex"
        data-aos="zoom-in"
        data-aos-delay="200"
      >
        <div class="team-member">
          <div class="member-img">
            <img src="assets/img/team/<%= team %>" class="img-fluid" alt="" />
          </div>
          <div class="member-info">
            <div class="social">
              <a href=""><i class="bi bi-twitter"></i></a>
              <a href=""><i class="bi bi-facebook"></i></a>
              <a href=""><i class="bi bi-instagram"></i></a>
              <a href=""><i class="bi bi-linkedin"></i></a>
            </div>
            <h4>Walter White</h4>
            <span>Chief Executive Officer</span>
          </div>
        </div>
      </div>
      <!-- End Team Member -->
      <%})%>
    </div>
  </div>
</section>
<!-- End Team Section -->
